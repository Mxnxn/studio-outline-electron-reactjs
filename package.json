{
  "name": "finest",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "author": "Mxnxn Inc.",
  "description": "Fin'est is estimator for architects and interior designers",
  "main": "build/electron.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.2",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "exceljs": "^4.2.0",
    "express": "^4.17.1",
    "express-logging": "^1.1.1",
    "history": "^5.0.0",
    "knex": "^0.21.18",
    "log4js": "^6.3.0",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sqlite3": "^5.0.2",
    "wait-on": "^5.2.1",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "elec-start": "electron .",
    "elec-build": "npm run build && electron-builder ",
    "elec-react": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.finest.app",
    "compression": "normal",
    "files": [
      "build/**/*",
      "data/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "data"
    },
    "extraFiles": {
      "from": "build",
      "to": "build"
    }
  },
  "devDependencies": {
    "electron": "^12.0.0",
    "electron-builder": "^22.10.5"
  }
}
